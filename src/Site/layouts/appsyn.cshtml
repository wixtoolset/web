<?xml version="1.0" ?>
<feed xmlns="http://www.w3.org/2005/Atom" xmlns:as="http://appsyndication.org/2006/appsyn">
  <title type="text">WiX Toolset @Model.Document.Version</title>
  <subtitle type="text">The WiX Toolset is a set of tools to create Windows installation packages from XML source code. The toolset supports a command line environment that developers may integrate into their build processes to build MSI and MSM setup packages.</subtitle>
  <as:application type="application/octet-stream">65E893AD-EDD5-4E7D-80CA-F0F50F383532</as:application>
  <updated>@Model.Document.StandardUtcDate</updated>
  <id>@Model.Document.Url</id>
  <link rel="self" type="application/atom+xml" href="@Model.Document.Url" />
  <generator version="0.1">wixweb build</generator>

  @foreach (var release in Model.Document.Releases)
  {
    var file = @release.Files[0];
    <entry>
      <title>WiX toolset @release.Title</title>
      <id>@release.Url</id>
      <author>
        <name>wixtoolset</name>
        <uri>http://twitter.com/wixtoolset</uri>
        <email>wix-users@lists.wixtoolset.org</email>
      </author>
      <link rel="alternate" href="@release.Url" />
      <link rel="enclosure" href="@(Model.Site.RootUrl)downloads/@release.Title/@file.Name" length="@file.Size" type="@file.ContentType" />
      <content type="html">&lt;p&gt;Change list:&lt;/p&gt; @release.Content</content>
      <as:version>@release.Title</as:version>
      <updated>@release.StandardUtcDate</updated>
</entry>
  }
</feed>